#include <iostream>
using namespace std;
void swappointer(int* a,int* b){
    int temp=*a;
    *a=*b;
    *b=temp;
}

int main() {
    int x=4,y=5;
    cout<<"the value of x and y before swapping is "<<endl<<x<<endl<<y<<endl;
    swappointer(&x,&y);
    cout<<"the value of x after swapping is  "<<x<<endl<<"the value of y after swapping is "<<y<<endl;



   return 0;
}
